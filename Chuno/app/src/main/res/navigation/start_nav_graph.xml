<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/start_nav_graph"
    app:startDestination="@id/permissionFragment">
    <action
        android:id="@+id/action_room_list_to_rankFragment"
        app:destination="@id/navigation" />
    <action
        android:id="@+id/action_roomListFragment_to_searchRoomFragment"
        app:destination="@id/navigation" />
    <action
        android:id="@+id/action_room_list_to_myPageFragment"
        app:destination="@id/navigation" />
    <action
        android:id="@+id/action_room_list_to_shopFragment"
        app:destination="@id/navigation" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.leesfamily.chuno.start.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_inputInfoFragment"
            app:destination="@id/inputInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/inputInfoFragment"
        android:name="com.leesfamily.chuno.start.InputInfoFragment"
        android:label="InputInfoFragment"
        tools:layout="@layout/fragment_input_info">
        <action
            android:id="@+id/action_inputInfoFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/permissionFragment"
        android:name="com.leesfamily.chuno.start.PermissionFragment"
        android:label="PermissionFragment"
        tools:layout="@layout/fragment_permission">
        <action
            android:id="@+id/action_permissionFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.leesfamily.chuno.room.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/home_fragment"/>

    <fragment
        android:id="@+id/game_view"
        android:name="com.leesfamily.chuno.game.GameViewFragment"
        android:label="GameViewFragment"
        tools:layout="@layout/fragment_game_view" />



    <navigation android:id="@+id/navigation"
        app:startDestination="@id/shop">
        <fragment
            android:id="@+id/search_room"
            android:name="com.leesfamily.chuno.room.search.SearchRoomFragment"
            android:label="SearchRoomFragment"
            tools:layout="@layout/fragment_search_room" />
        <fragment
            android:id="@+id/rankFragment"
            android:name="com.leesfamily.chuno.room.rank.RankFragment"
            android:label="RankFragment"
            tools:layout="@layout/fragment_rank" />
        <fragment
            android:id="@+id/shop"
            android:name="com.leesfamily.chuno.room.shop.ShopFragment"
            android:label="fragment_shop"
            tools:layout="@layout/fragment_shop" />
        <fragment
            android:id="@+id/room_list"
            android:name="com.leesfamily.chuno.room.roomlist.RoomListFragment"
            android:label="RoomListFragment"
            tools:layout="@layout/fragment_room_list">
            <action
                android:id="@+id/action_roomListFragment_to_searchRoomFragment"
                app:destination="@id/search_room" />
            <action
                android:id="@+id/action_room_list_to_shopFragment"
                app:destination="@id/shop" />
            <action
                android:id="@+id/action_room_list_to_myPageFragment"
                app:destination="@id/my_page" />
            <action
                android:id="@+id/action_room_list_to_rankFragment"
                app:destination="@id/rankFragment" />
        </fragment>
        <fragment
            android:id="@+id/my_page"
            android:name="com.leesfamily.chuno.room.page.MyPageFragment"
            android:label="fragment_my_page"
            tools:layout="@layout/fragment_my_page" />
    </navigation>

    <include app:graph="@navigation/nav_graph" />

</navigation>